{
  "name": "nuxt-3-stater",
  "version": "0.1.0",
  "description": "a Nuxt 3 application",
  "repository": {
    "type": "git",
    "url": "git://gitlab.com/g5411/gptc-web"
  },
  "author": "nam3886",
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "build:production": "nuxt build --dotenv .env.production",
    "build:staging": "nuxt build --dotenv .env.staging",
    "build:testing": "nuxt build --dotenv .env.testing",
    "dev": "nuxt dev --dotenv .env.development",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "serve": "serve dist/",
    "postinstall": "husky install",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "prepare": "husky install",
    "clean": "rm -rf .nuxt dist .output",
    "generate:locales": "node tools/translator.js ./locales en.yml",
    "type-check": "nuxi typecheck ."
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@iconify/json": "^2.1.135",
    "@intlify/nuxt3": "^0.2.4",
    "@nuxt/test-utils-edge": "^3.4.2-28030302.886350a2",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@pinia/nuxt": "^0.4.6",
    "@types/qs": "^6.9.7",
    "@vue-macros/volar": "^0.5.9",
    "@vueuse/core": "^9.11.1",
    "@vueuse/nuxt": "^9.11.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "husky": "^8.0.2",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.0.3",
    "nuxt": "^3.4.1",
    "nuxt-windicss": "^2.6.0",
    "pinia": "^2.0.29",
    "postcss": "8.4.14",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "serve": "^13.0.2",
    "three": "^0.143.0",
    "translate": "^1.4.1",
    "unplugin-icons": "^0.14.14",
    "unplugin-vue-components": "^0.22.9",
    "vitest": "^0.28.3",
    "vue-tsc": "^1.2.0"
