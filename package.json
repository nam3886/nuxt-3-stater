    "@tanstack/vue-query": "^4.24.4",
    "@vue-macros/nuxt": "^0.2.9",
    "@vueuse/router": "^9.11.1",
    "moment": "^2.29.4",
    "moment-range": "^4.0.2",
    "nuxt-lodash": "^2.4.1",
    "nuxt-typed-router": "^3.0.7",
    "qs": "^6.11.0",
    "v-viewer": "^3.0.11",
